{
	"name": "create-nodesand",
	"type": "module",
	"version": "1.2.0",
	"description": "Super Simple Node.js Sandbox",
	"author": "Jooy2 <jooy2.contact@gmail.com>",
	"license": "MIT",
	"homepage": "https://github.com/jooy2/nodesand",
	"repository": {
		"type": "git",
		"url": "https://github.com/jooy2/nodesand"
	},
	"main": "dist/index.js",
	"bin": "dist/index.js",
	"exports": {
		".": "./dist/index.js"
	},
	"scripts": {
		"test": "npm run build && node dist/index.js",
		"build": "npm run format:fix && tsc && npm run minify",
		"minify": "terser-glob 'dist/**/*.js' --config-file terser.config.json",
		"format": "prettier .",
		"format:fix": "prettier . --write"
	},
	"keywords": [
		"node",
		"nodejs",
		"test",
		"serve",
		"sandbox",
		"html"
	],
	"devDependencies": {
		"@eslint/js": "^9.34.0",
		"@types/node": "^24.3.0",
		"@typescript-eslint/parser": "^8.41.0",
		"eslint": "^9.34.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-n": "^17.21.3",
		"globals": "^16.3.0",
		"jiti": "^2.5.1",
		"prettier": "^3.6.2",
		"terser-glob": "^1.1.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.41.0"
	}
}
